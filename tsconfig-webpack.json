{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "strict": true,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noImplicitAny": true,
        "newLine": "LF"
    }
}